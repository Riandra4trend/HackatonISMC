generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  user_id    Int     @id @default(autoincrement())
  username   String  @unique @db.VarChar(256)
  password   String  @db.VarChar(256)
  is_manager Boolean @default(false)
}
